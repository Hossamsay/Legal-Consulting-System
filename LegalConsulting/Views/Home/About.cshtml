@model IEnumerable<LegalConsulting.ViewModel.HiringDateGroup>
@{
    ViewBag.Title = "Lawyer Body Statistics";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}<br /><br /><br />
    <h2>Lawyer Body Statistics</h2>
<table class="table">
    <tr>
        <th>
            Hiring Date
        </th>
        <th>
            Lawyers
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HiringDate)
            </td>
            <td>
                @item.LawyerCount
            </td>
        </tr>
    }
</table>

